{% for provider in s3_provider_credentials %}
[{{ provider }}]
type = s3
provider = {{ provider }}
access_key_id = {{ s3_provider_credentials[provider].access_key }}
secret_access_key = {{ s3_provider_credentials[provider].secret_key }}
{% if s3_provider_credentials[provider].endpoint is defined %}
endpoint = {{ s3_provider_credentials[provider].endpoint }}
{% endif %}
{% endfor %}